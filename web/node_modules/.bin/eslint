#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/BMAD method/BMAD 4/node_modules/.pnpm/eslint@9.39.2/node_modules/eslint/bin/node_modules:/mnt/e/BMAD method/BMAD 4/node_modules/.pnpm/eslint@9.39.2/node_modules/eslint/node_modules:/mnt/e/BMAD method/BMAD 4/node_modules/.pnpm/eslint@9.39.2/node_modules:/mnt/e/BMAD method/BMAD 4/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/BMAD method/BMAD 4/node_modules/.pnpm/eslint@9.39.2/node_modules/eslint/bin/node_modules:/mnt/e/BMAD method/BMAD 4/node_modules/.pnpm/eslint@9.39.2/node_modules/eslint/node_modules:/mnt/e/BMAD method/BMAD 4/node_modules/.pnpm/eslint@9.39.2/node_modules:/mnt/e/BMAD method/BMAD 4/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/eslint@9.39.2/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/eslint@9.39.2/node_modules/eslint/bin/eslint.js" "$@"
fi
